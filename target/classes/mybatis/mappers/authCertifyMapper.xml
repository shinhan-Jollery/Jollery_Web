<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shinhan.authCertify">

    <insert id="insertAccount" parameterType="com.shinhan.VO.MembersDTO">
        UPDATE members
		SET member_is_artist = #{member_is_artist}
		WHERE member_id = #{member_id}
    </insert>

</mapper>
